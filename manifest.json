{
  "name": "Stop watching YouTube.",
  "description": "Don't waste your time anymore.",
  "version": "1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"]
    }
  ],
  "permissions": ["tabs", "activeTab", "webNavigation", "<all_urls>"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
